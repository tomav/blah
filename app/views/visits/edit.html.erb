<h1>Editing visit</h1>

<% form_for(@visit) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :link_id %><br />
    <%= f.text_field :link_id %>
  </p>
  <p>
    <%= f.label :ip_address %><br />
    <%= f.text_area :ip_address %>
  </p>
  <p>
    <%= f.label :referer %><br />
    <%= f.text_area :referer %>
  </p>
  <p>
    <%= f.label :browser %><br />
    <%= f.text_area :browser %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @visit %> |
<%= link_to 'Back', visits_path %>